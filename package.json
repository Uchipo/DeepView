{
  "name": "deepview",
  "version": "1.0.0",
  "description": "",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "npm --workspace server run dev",
    "dev:client": "npm --workspace client run dev",
    "build": "npm run build:server && npm run build:client",
    "build:server": "npm --workspace server run build",
    "build:client": "npm --workspace client run build",
    "lint": "eslint \"packages/**/*.{ts,tsx}\" --fix",
    "start": "concurrently  \"cd packages/client && npm --workspace client start\" \"cd packages/server && npm --workspace server start\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Uchipo/DeepView.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Uchipo/DeepView/issues"
  },
  "homepage": "https://github.com/Uchipo/DeepView#readme",
  "dependencies": {
    "concurrently": "^8.2.2",
    "eslint": "^8.x",
    "prettier": "^2.x"
  }
}
